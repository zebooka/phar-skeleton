{
    "name": "zebooka/phar-skeleton",
    "description": "Skeleton to build phar applications using Docker.",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Zebooka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Zebooka\\": "tests/unit/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-mbstring": "*",
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    }
}
